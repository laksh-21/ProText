<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MenuIconButtons" parent="Widget.Material3.MaterialTimePicker.ImageButton">
        <item name="android:tint">?attr/colorOnBackground</item>
        <item name="android:background">?attr/selectableItemBackground</item>
    </style>
    <style name="ListTitleTextExpanded" parent="TextAppearance.Material3.HeadlineLarge">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">2</item>
        <item name="android:textColor">?attr/colorOnBackground</item>
        <item name="android:textSize">@dimen/pageTitleExpanded</item>
    </style>
    <style name="ListTitleTextCollapsed" parent="TextAppearance.Material3.HeadlineLarge">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">2</item>
        <item name="android:textColor">?attr/colorOnBackground</item>
        <item name="android:textSize">@dimen/pageTitleCollapsed</item>
    </style>
    <style name="PageContentText" parent="TextAppearance.Material3.BodyMedium">
        <item name="android:textColor">?attr/colorOnBackground</item>
    </style>
    <style name="FolderListTitleText" parent="TextAppearance.Material3.LabelLarge">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">2</item>
        <item name="android:textColor">?attr/colorOnSecondary</item>
    </style>
    <style name="ListRecyclerView">
        <item name="android:overScrollMode">never</item>
    </style>
    <style name="FolderListItemCard" parent="Widget.Material3.CardView.Filled">
        <item name="cardCornerRadius">8dp</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:checkable">true</item>
        <item name="cardBackgroundColor">?attr/colorSurface</item>
    </style>
    <style name="ListItemTitleText" parent="TextAppearance.Material3.BodyLarge">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">2</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>
    <style name="ListItemBodyText" parent="TextAppearance.Material3.BodyMedium">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>
    <style name="ListItemBodyCopiedText" parent="TextAppearance.Material3.BodyMedium">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">4</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>
    <style name="ListItemTimeText" parent="TextAppearance.Material3.BodySmall">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
        <item name="android:alpha">0.5</item>
    </style>
    <style name="TextButton" parent="Widget.Material3.Button.TextButton">

    </style>
</resources>